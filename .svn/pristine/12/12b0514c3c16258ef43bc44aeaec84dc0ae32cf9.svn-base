<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <title>众彩物流</title>
    <meta http-equiv="content-type" content="text/html;charset=utf-8"/>
	<meta http-equiv="Cache-Control" content="no-store" />
	<meta http-equiv="Pragma" content="no-cache" />
	<meta http-equiv="Expires" content="0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="description" content="CISDIOA"/>
    <meta name="author" content="CISDIOA"/>
    <!-- Import resource file -->
    #parse(${resource_path})
	<script type="text/javascript" src="${rc.contextPath}/resources/js/basic/commodityClassify/commodityClassify.js"></script>
	
	<script type="text/javascript">
$(function(){ 	
});

function searchData(){
	var queryParams = {
	};
	$('#dg').datagrid('options').queryParams = queryParams;
	$("#dg").datagrid('load');
}
</script>
</head>
<body style="background-color: #fff">
    <div class="easyui-panel" style="height:100%;width:100%">
    	<form method="" id="baseForm">
			<input type="hidden" name="workStation.id" id="id" value="$!{workStation.id}"/>
            <div class="cell">
        		<table width="100%">
    				<tr>
						<th style="width:15%;">
    						<span style='color:red'>*</span>工位编号：
                        </th>
    					<td style="width:35%;">
							<input name="workStation.workCode" id="workCode" style="width:130px" class="easyui-textbox" reg="Require" tip="请填写工位编号"></input>
                        </td>
						<th style="width:15%;">
    						<span style='color:red'>*</span>工位名称：
                        </th>
    					<td style="width:35%;">
							<input name="workStation.workName" id="workName" style="width:130px" class="easyui-textbox" reg="Require" tip="请填写工位名称 "></input>
                        </td>
    				</tr>
					<tr>
						<th style="width:15%;">
    						<span style='color:red'>*</span>IP分配：
                        </th>
    					<td style="width:35%;">
							<input name="workStation.workIp" id="workIp" style="width:130px" class="easyui-textbox" reg="Require" tip="请填写工位IP"/>
                        </td>
						<th style="width:15%;">
    						工位描述：
                        </th>
    					<td style="width:35%;">
							<input name="workStation.description" id="description" style="width:130px" class="easyui-textbox"/>
                        </td>
    				</tr>
					<tr>
    					<td>
							&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        </td>
						<td>
							&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        </td>
    				</tr>
					</br>
                </table>
    		</div>
			<div style="text-align:center;margin-left:auto;margin-right:auto;">
    			<a href="#" class="easyui-linkbutton" data-options="" style="width:80px;" onclick="saveNewWorkStation('baseForm');">新增</a>
    			<a href="#" class="easyui-linkbutton" data-options="" style="width:80px;" onclick="window.parent.closeWinForm();">关闭</a>
            </div>
        </form>
    </div>
</body>
</html>